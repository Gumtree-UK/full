@isTest
class TestFiqasPDFViewController {
    
    static testmethod void test() {
        Profile p = [select Id From Profile Where Name = 'System Administrator' limit 1];
        User u = new User(ProfileId = p.Id, alias = 'u2', email = 'fiqasPDFViewController@test.com', emailencodingkey = 'UTF-8', 
                            FirstName = 'fiqasPDFViewController' , lastname = 'TMPC', languagelocalekey = 'en_US',
                            localesidkey = 'en_US',timezonesidkey = 'America/Los_Angeles', CommunityNickname = 'FiqasPDFView',
                            isActive = true, username = 'fiqasPDFViewController@test.com');
        insert u;
        Account acc = new Account(Name = 'Test Account Fiqas', 
                                  BillingStreet = 'Fake Street', 
                                  BillingCity = 'Fake City',
                                  BillingCountry = 'GB',
                                  BillingPostalCode = 'TW9');
        insert acc;
        Invoices__c inv = new Invoices__c(Invoice_Date__c = System.today(), Account__c = acc.Id);
        insert inv;
        Fiqas_Settings__c fiqasSetting = new Fiqas_Settings__c(Name = 'Webservice', Http_header__c = 'test', Endpoint__c = 'test', User__c = 'testUser', 
                                            Key__c = 'testing123');
        insert fiqasSetting;
        Test.startTest();
        system.runAs(u) {
            Pagereference pdfPage = Page.FiqasPDFViewPage;
            pdfPage.getParameters().put('id', null);
            pdfPage.getParameters().put('invId', inv.Id);
            Test.setCurrentPageReference(pdfPage);
            FiqasPDFViewController con = new FiqasPDFViewController();
            pdfPage.getParameters().put('id', [select Name From Invoices__c Where Id = : inv.Id].get(0).Name);
            con = new FiqasPDFViewController();
            con.mockBody = '<?xml version="1.0" encoding="utf-8"?>' +
            '<ArrayOfGetDocumentOut xmlns:xsi=' + '"http://www.w3.org/2001/XMLSchema-instance"' + 'xmlns:xsd=' + '"http://www.w3.org/2001/XMLSchema"' + 'xmlns=' + '"http://www.fiqas.nl/AdaptAPI/">' + 
            '<GetDocumentOut>' +
            '<PDFFile></PDFFile>' + 
            '</GetDocumentOut>' + 
            '</ArrayOfGetDocumentOut>';
            con.openPdfProcess();
        }
        system.assertEquals(1, [Select Id From Attachment Where ParentId = : inv.Id].size());
        Test.stopTest();
    }
}